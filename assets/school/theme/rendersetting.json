{
  "environmentMap": {
    "envmap": "cloudsky",
    "path": [
      "./skyboxes/cloudsky/posx.jpg",
      "./skyboxes/cloudsky/negx.jpg",
      "./skyboxes/cloudsky/posy.jpg",
      "./skyboxes/cloudsky/negy.jpg",
      "./skyboxes/cloudsky/posz.jpg",
      "./skyboxes/cloudsky/negz.jpg"
    ]
  },
  "light": {
