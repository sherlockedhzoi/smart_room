    "dof": {
      "enable": false,
      "focalDepth": 1,
      "focalLength": 24,
      "fstop": 0.9,
      "maxblur": 1,
      "threshold": 0.9,
      "gain": 1,
      "bias": 0.5,
      "dithering": 0.0001
